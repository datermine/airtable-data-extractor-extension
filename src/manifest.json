{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "Airtable Data Extractor",
  "short_name": "Airtable Data Extractor",
  "description": "Allows you to extract raw data (JSON or CSV) from an Airtable shared view.",
  "version": "1.0.0",
  "browser_action": {},
  "icons": {
    "34": "icon-34.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "*://airtable.com/*",
    "contextMenus",
    "downloads",
    "webRequest"
  ],
  "background": {
    "persistent": true,
    "scripts": ["airtable.bundle.js"]
  }
}
